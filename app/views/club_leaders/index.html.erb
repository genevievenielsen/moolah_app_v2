<h1>Listing club_leaders</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @club_leaders.each do |club_leader| %>
      <tr>
        <td><%= club_leader.first_name %></td>
        <td><%= club_leader.last_name %></td>
        <td><%= club_leader.title %></td>
        <td><%= link_to 'Show', club_leader %></td>
        <td><%= link_to 'Edit', edit_club_leader_path(club_leader) %></td>
        <td><%= link_to 'Destroy', club_leader, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Club leader', new_club_leader_path %>

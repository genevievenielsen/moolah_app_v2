<h1>Listing club_admins</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Club</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @club_admins.each do |club_admin| %>
      <tr>
        <td><%= club_admin.user_id %></td>
        <td><%= club_admin.club_id %></td>
        <td><%= link_to 'Show', club_admin %></td>
        <td><%= link_to 'Edit', edit_club_admin_path(club_admin) %></td>
        <td><%= link_to 'Destroy', club_admin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Club admin', new_club_admin_path %>
